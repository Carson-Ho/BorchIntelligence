<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent">




    <RelativeLayout
        android:id="@+id/title1"
        android:layout_width="match_parent"
        android:layout_height="@dimen/x43"
        android:background="@color/colorBackground"
        >


        <Button
            android:id="@+id/back"
            android:layout_alignParentLeft="true"
            android:layout_centerVertical="true"
            android:layout_marginLeft="@dimen/x21"
            android:layout_width="@dimen/x10"
            android:layout_height="@dimen/y13"
            android:background="@drawable/back"
            />


        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerInParent="true"
            android:text="购机向导"
            android:textSize="@dimen/x22"
            android:textColor="@color/color_default"/>



    </RelativeLayout>


    <ImageView
        android:id="@+id/process1"
        android:layout_below="@+id/title1"
        android:layout_marginTop="@dimen/y10"
        android:layout_centerInParent="true"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@drawable/step2"/>


    <TextView
        android:id="@+id/title2"
        android:layout_below="@+id/process1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/y10"
        android:layout_centerHorizontal="true"
        android:text="请选择模具参数"
        android:textSize="@dimen/x16"
        android:textColor="@color/colorTab_selected"/>



    <RelativeLayout
        android:id="@+id/line"
        android:layout_below="@+id/title2"
        android:layout_marginTop="@dimen/y10"
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="@color/colorline"
        >
    </RelativeLayout>



    <RelativeLayout
        android:id="@+id/choese10"
        android:layout_below="@+id/line"
        android:layout_width="match_parent"
        android:layout_height="@dimen/y25"
        android:focusable="true"
        android:focusableInTouchMode="true">


        <TextView
            android:id="@+id/Size"
            android:layout_alignParentLeft="true"
            android:layout_centerVertical="true"
            android:layout_marginTop="@dimen/y8"
            android:layout_marginLeft="@dimen/x21"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="模具尺寸"
            android:textSize="@dimen/x14"
            android:textColor="@color/colorparameter1"/>

    </RelativeLayout>




    <RelativeLayout
        android:id="@+id/choese11"
        android:layout_below="@+id/choese10"
        android:layout_marginTop="@dimen/y1"
        android:layout_width="match_parent"
        android:layout_height="@dimen/y25"
        android:focusable="true"
        android:focusableInTouchMode="true">


        <TextView
            android:layout_centerVertical="true"
            android:layout_marginLeft="@dimen/x21"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/parameter_productLength"
            android:textSize="@dimen/x14"
            android:textColor="@color/colorparameter1"/>



        <EditText
            android:id="@+id/editText3"
            android:layout_centerVertical="true"
            android:layout_marginLeft="@dimen/x116"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:hint="请填写"
            android:textSize="@dimen/x14"
            android:textColor="@color/colorparameter2"/>


    </RelativeLayout>


    <RelativeLayout
        android:id="@+id/choese12"
        android:layout_below="@+id/choese11"
        android:layout_marginTop="@dimen/y1"
        android:layout_width="match_parent"
        android:layout_height="@dimen/y25"
        android:focusable="true"
        android:focusableInTouchMode="true">


        <TextView
            android:layout_centerVertical="true"
            android:layout_marginLeft="@dimen/x21"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/parameter_productWidth"
            android:textSize="@dimen/x14"
            android:textColor="@color/colorparameter1"/>



        <EditText
            android:id="@+id/editText4"
            android:layout_centerVertical="true"
            android:layout_marginLeft="@dimen/x116"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:hint="请填写"
            android:textSize="@dimen/x14"
            android:textColor="@color/colorparameter2"/>


    </RelativeLayout>



    <RelativeLayout
        android:id="@+id/choese13"
        android:layout_below="@+id/choese12"
        android:layout_marginTop="@dimen/y1"
        android:layout_width="match_parent"
        android:layout_height="@dimen/y25"
        android:focusable="true"
        android:focusableInTouchMode="true">


        <TextView
            android:layout_centerVertical="true"
            android:layout_marginLeft="@dimen/x21"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/parameter_productHeight"
            android:textSize="@dimen/x14"
            android:textColor="@color/colorparameter1"/>



        <EditText
            android:id="@+id/editText5"
            android:layout_centerVertical="true"
            android:layout_marginLeft="@dimen/x116"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:hint="请填写"
            android:textSize="@dimen/x14"
            android:textColor="@color/colorparameter2"/>


    </RelativeLayout>
</RelativeLayout>