<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <RelativeLayout
        android:id="@+id/title1"
        android:layout_width="match_parent"
        android:layout_height="@dimen/x43"
        android:background="@color/colorBackground"
    >


        <Button
            android:id="@+id/back"
            android:layout_alignParentLeft="true"
            android:layout_centerVertical="true"
            android:layout_marginLeft="@dimen/x21"
            android:layout_width="@dimen/x10"
            android:layout_height="@dimen/y13"
            android:background="@drawable/back"
            />


        <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerInParent="true"
        android:text="购机向导"
        android:textSize="@dimen/x22"
        android:textColor="@color/color_default"/>



    </RelativeLayout>


    <ImageView
        android:id="@+id/process1"
        android:layout_below="@+id/title1"
        android:layout_marginTop="@dimen/y10"
        android:layout_centerInParent="true"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@drawable/step1"/>


    <TextView
        android:layout_below="@+id/process1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/y10"
        android:layout_centerInParent="true"
        android:text="请选择塑料制品参数"
        android:textSize="@dimen/x16"
        android:textColor="@color/colorTab_selected"/>

    <RelativeLayout
        android:id="@+id/line"
        android:layout_below="@+id/title1"
        android:layout_marginTop="@dimen/x75"
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="@color/colorline"
        >
    </RelativeLayout>


    <RelativeLayout
        android:id="@+id/choese1"
        android:layout_below="@+id/line"
        android:layout_width="match_parent"
        android:layout_height="@dimen/y31">


        <TextView

            android:layout_alignParentLeft="true"
            android:layout_centerVertical="true"
            android:layout_marginLeft="@dimen/x21"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/parameter_productType"
            android:textSize="@dimen/x14"
            android:textColor="@color/colorparameter1"/>



        <TextView
            android:id="@+id/text2"
            android:layout_centerVertical="true"
            android:layout_marginLeft="@dimen/x116"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="请选择"
            android:textSize="@dimen/x14"
            android:textColor="@color/colorparameter2"/>


    </RelativeLayout>

</RelativeLayout>




